{
    "type": "robo-tasks",
    "name": "sweetchuck/robo-phpunit",
    "description": "Robo task to run PHPUnit related commands",
    "keywords": [
        "robo-tasks",
        "phpunit"
    ],
    "authors": [
        {
            "name": "Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "support": {
        "source": "https://github.com/Sweetchuck/robo-phpunit",
        "issues": "https://github.com/Sweetchuck/robo-phpunit/issues"
    },
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.3",
        "ext-dom": "*",
        "ext-pcre": "*",
        "consolidation/robo": "^2.0",
        "danielstjules/stringy": "^3.1",
        "sweetchuck/cli-cmd-builder": "^0.0",
        "sweetchuck/utils": "1.x-dev"
    },
    "require-dev": {
        "ext-json": "*",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.1",
        "squizlabs/php_codesniffer": "^3.5",
        "sweetchuck/codeception-module-robo-task-runner": "dev-master",
        "sweetchuck/git-hooks": "^2.x-dev",
        "sweetchuck/robo-git": "1.x-dev",
        "sweetchuck/robo-phpcs": "1.x-dev",
        "symfony/error-handler": "^4.0 || ^5.0",
        "symfony/console": "^4.4 || ^5.0",
        "symfony/finder": "^4.0 || ^5.0",
        "webmozart/path-util": "^2.3"
    },
    "suggest": {
        "phpunit/phpunit": "The 'phpunit' executable is required."
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\Robo\\PHPUnit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\Robo\\PHPUnit\\Test\\": "tests/_support/",
            "Sweetchuck\\Robo\\PHPUnit\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\Robo\\PHPUnit\\Tests\\Unit\\": "tests/unit/",
            "Sweetchuck\\Robo\\PHPUnit\\TestsFixtures\\": "tests/_data/fixtures/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {}
}
